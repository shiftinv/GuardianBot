version: '3'
services:
  main:
    image: shiftinv/guardianbot
    restart: unless-stopped
    environment:
      DISCORD_DEBUG: '1'
      DISCORD_PREFIX: '?'
      DISCORD_DATA_DIR: './data'
      DISCORD_TOKEN: '<EMPTY>'
      DISCORD_GUILD_ID: '<EMPTY>'
      DISCORD_MUTED_ROLE_ID: '<EMPTY>'
      DISCORD_MOD_ROLE_IDS: '<EMPTY>,<EMPTY>,...'
      # DISCORD_ENABLE_OWNER_EVAL: '1'
    volumes:
      - './_data:/app/data'
